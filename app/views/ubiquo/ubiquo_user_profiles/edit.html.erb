<h1><%= render :partial => 'title' %></h1>
<h2><%= render :partial => "submenu" %></h2>

<%= error_messages_for 'chapter' %>

<%= ubiquo_form_for(@ubiquo_user, :url => ubiquo.ubiquo_user_profile_path, :html => { :method => :put, :multipart => true }) do |form| %>
  <%= render :partial => 'form', :locals => { :form => form } %>
  <% form.submit_group do %>
    <%= form.update_button %>
    <%= form.back_button %>
  <% end %>
<% end %>

<% content_for :sidebar do %>
  <%= help_block_sidebar(t("ubiquo.helpmsg.authentication.ubiquouserprofile.edit")) %>
<% end %>
